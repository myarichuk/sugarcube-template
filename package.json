{
  "name": "sugarcube-template",
  "version": "1.0.0",
  "description": "template for a Twine/Sugarcube game",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production && ts-node setup-tweego.ts && webpack --mode production && ts-node ./run-tweego.ts src/story -o dist/index.html --head=src/head_content.html",
    "start": "cross-env NODE_ENV=development && ts-node setup-tweego.ts && webpack --mode development && ts-node ./run-tweego.ts src/story -o dist/index.html --head=src/head_content.html && webpack --mode development --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "install-tweego": "ts-node setup-tweego.ts"
  },
  "keywords": [
    "twine",
    "sugarcube",
    "starter-kit",
    "boilerplate",
    "tweego"
  ],
  "author": "Michaek Yarichuk",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/twine-sugarcube": "^2.36.5",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.44.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass": "^9.0.0",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "node-fetch": "^3.3.1",
    "unzipper": "^0.10.14"
  }
}
